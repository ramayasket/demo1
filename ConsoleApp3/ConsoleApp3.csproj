<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoWarn>0162;1822;1825;1998;CS0649;0219;0164;8618;2208;NU1803;8981;</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Kw.Common" Version="2.1.8.6" />
  </ItemGroup>

  <ItemGroup>
    <None Update="input1.log">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="input2.log">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
